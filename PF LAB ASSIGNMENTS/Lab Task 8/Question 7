#include <stdio.h>
int main() {
    int rows = 5;
   int i,j,k,l;
    for (i = 0; i < rows; i++) {
        for ( j = 0; j < 3; j++) {
            for ( k = 0; k < rows - i - 1; k++) {
                printf(" ");
            }
            printf("*");

            if (j < 2) {
                for (l = 0; l < 5; l++) {
                    printf(" ");
                }
            }
        }
        printf("\n");
    }

    return 0;
}
