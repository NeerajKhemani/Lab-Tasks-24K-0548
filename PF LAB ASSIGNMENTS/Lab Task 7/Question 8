#include <stdio.h>
int main() {
    char expr[100];
    int i = 0, num = 0, result = 0;
    char operator = '+';

    printf("Enter an expression (e.g., 20+10-30): ");
    fgets(expr, sizeof(expr), stdin);

    while (expr[i] != '\0') {
        if (expr[i] >= '0' && expr[i] <= '9') {
            num = num * 10 + (expr[i] - '0');
        } else if (expr[i] == '+' || expr[i] == '-') {
            if (i == 0 || (expr[i - 1] < '0' || expr[i - 1] > '9')) {
                printf("Invalid expression\n");
                return 0;
            }
            if (operator == '+') {
                result += num;
            } else if (operator == '-') {
                result -= num;
            }
            operator = expr[i];
            num = 0;
        } else if (expr[i] == '\0') {
            if (i == 0 || (expr[i - 1] < '0' || expr[i - 1] > '9')) {
                printf("Invalid expression\n");
                return 0;
            }
            break;
        }
        i++;
    }

    if (operator == '+') {
        result += num;
    } else if (operator == '-') {
        result -= num;
    }

    printf("Result: %d\n", result);
    return 0;
}
