#include <stdio.h>

void swapIntegers(int x, int y) {
    int temp = x;
    x = y;
    y = temp;
     printf("After swaping: x = %d, y = %d\n", x, y);
}

int main() {
    int x, y;

    printf("Enter the first integer: ");
    scanf("%d", &x);

    printf("Enter the second integer: ");
    scanf("%d", &y);

    printf("Before swaping: x = %d, y = %d\n", x, y);

    swapIntegers(x, y);
   
     return 0;
}
