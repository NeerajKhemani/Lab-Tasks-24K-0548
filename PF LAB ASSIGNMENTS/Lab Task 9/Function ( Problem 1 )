#include <stdio.h>
int multiply(int a, int b) {
    return a * b;
}
int main() {
    int num1, num2, result;
    
    printf("Enter the first number: ");
    scanf("%d", &num1);
    
    printf("Enter the second number: ");
    scanf("%d", &num2);
    
    result = multiply(num1, num2);
    
    printf("The product is: %d\n", result);
    
    return 0;
}
